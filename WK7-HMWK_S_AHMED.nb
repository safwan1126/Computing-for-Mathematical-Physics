Cell[TextData[{
 StyleBox["In the input cell below the function ",
  FontSize->16,
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "[", 
    RowBox[{"x", ",", "y"}], "]"}], TraditionalForm]],
