 StyleBox[" is defined as \n\[LineSeparator]\[FilledSmallCircle]   ",
  FontSize->16,
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"g", "[", 
     RowBox[{"x", ",", "y"}], "]"}], "   ", "=", "  ", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"Cos", "[", 
       RowBox[{
        FractionBox["1", "2"], 
        SqrtBox[
         RowBox[{
          SuperscriptBox["x", "2"], "+", 
          SuperscriptBox["y", "2"]}]]}], "]"}], "2"], "  ", 
     SuperscriptBox[
      RowBox[{"Cos", "[", " ", 
       RowBox[{"2", 
        RowBox[{"ArcTan", "[", 
         RowBox[{"x", ",", "y"}], "]"}]}], " ", "]"}], "2"], " ", 
     RowBox[{"\[CapitalTheta]", "(", 
      RowBox[{
       RowBox[{"3", "\[Pi]"}], "-", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["y", "2"]}]]}], ")"}], " ", 
     RowBox[{
      RowBox[{"\[CapitalTheta]", "(", 
       RowBox[{
        SqrtBox[
         RowBox[{
          SuperscriptBox["x", "2"], "+", 
          SuperscriptBox["y", "2"]}]], "-", "\[Pi]"}], ")"}], " ", "."}]}]}], 
   TraditionalForm]],
  FontSize->16,
  FontColor->RGBColor[0, 0, 1],ExpressionUUID->
  "c5703ebe-7bb5-4589-882f-2fd86103c2c7"],
 StyleBox["\n\[LineSeparator]A 3D surface plot is also provided to show the \
form of the function. \[LineSeparator]\[LineSeparator]In this question we \
seek to perform the following integral\[LineSeparator]by so-called Monte \
Carlo methods\[LineSeparator]\[LineSeparator]\[FilledSmallCircle]   ",
  FontSize->16,
  FontColor->RGBColor[0, 0, 1]],
 Cell[BoxData[
  FormBox[
   RowBox[{"\[ScriptCapitalI]", " ", "=", " ", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["y", "min"], 
      SubscriptBox["y", "max"]], 
