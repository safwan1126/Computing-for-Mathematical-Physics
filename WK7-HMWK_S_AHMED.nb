      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["x", "min"], 
       SubscriptBox["x", "max"]], 
      RowBox[{
       RowBox[{"g", "[", 
        RowBox[{"x", ",", "y"}], "]"}], 
       RowBox[{"\[DifferentialD]", "x"}], 
       RowBox[{"\[DifferentialD]", "y"}]}]}]}]}], TraditionalForm]],
