    "}"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
     "nextNPoints", ",", "\[IndentingNewLine]", "nextOnePointApproximation", 
      ",", "\[IndentingNewLine]", "nextAvg", ",", "\[IndentingNewLine]", 
      "nextErr", ",", "\[IndentingNewLine]", "nextAvgOfSquares"}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"nextNPoints", "=", "\[IndentingNewLine]", 
      RowBox[{"nPoints", "+", "1"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"nextOnePointApproximation", "=", "\[IndentingNewLine]", 
      RowBox[{"onePointApproximation", "[", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"nextAvg", "=", "\[IndentingNewLine]", 
      RowBox[{
       FractionBox["1", "nextNPoints"], "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"avg", " ", "*", " ", "nPoints"}], " ", "+", 
         "nextOnePointApproximation"}], " ", ")"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"nextAvgOfSquares", "=", "\[IndentingNewLine]", 
      RowBox[{
       FractionBox["1", "nextNPoints"], "*", 
